import pickle

class Vehiculo:
  categoria = ""
  precio = 0.0
  
  def __init__(self, categoria, precio):
      self.categoria = categoria
      self.precio    = precio
      
  def getCategoria(self):
      return self.categoria
      
v1 = Vehiculo ("cabrio", 40.000)

